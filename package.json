{
  "name": "qi",
  "version": "0.1.0",
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "svelte": "^5.0.0",
    "svelte-adapter-bun": "^0.5.2",
    "vite": "^6.2.6"
  },
  "private": true,
  "scripts": {
    "dev": "npm-run-all --print-label --parallel --race dev-ui dev-hub",
    "dev-ui": "bun run --filter './addons/*/ui_src' dev",
    "build": "npm-run-all --serial build-ui hub",
    "build-ui": "bun run --filter './addons/*/ui_src' build",
    "preview-ui": "bun run --filter './addons/*/ui_src' preview",
    "dev-hub": "./.venv/Scripts/activate; python -m hub.main --dev-mode",
    "hub": "./.venv/Scripts/activate; python -m hub.main"
  },
  "type": "module",
  "workspaces": [
    "./addons/*/ui_src"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2"
  }
}
